;AACA;;AAEA,MAAA,EAAA,CAAA,EAAA,CAAA,UAAA,CAAA,KAAA,IAAA,EAAA,GAAA,CAAA,MAAA,KAAA,QAAA,CAAA,GAAA,CAAA,UAAA,CAAA,EAAA,UAAA,CAAA,SAAA,EAAA,UAAA,CAAA,GAAA,IAAA;AACA,cAAA,OAAA,GAAA,GAAA,CAAA,UAAA,EAAA;AAEA,QAAA,OAAA,CAAA,OAAA,CAAA,MAAA,IAAA;AACA,cAAA,MAAA,CAAA,IAAA,KAAA,OAAA,EAAA;AACA,gBAAA,CAAA,GAAA,MAAA,CAAA,GAAA,CAAA,IAAA,EAAA;AACA,gBAAA,IAAA,GAAA,KAAA,kBAAA,CAAA,CAAA,CAAA,SAAA,CAAA;AACA,iBAAA,gBAAA,CAAA,IAAA,CAAA,IAAA;AACA,iBAAA,YAAA,CAAA,IAAA,CAAA,MAAA,CAAA,GAAA,CAAA,EAAA;;AAEA,oBAAA,CAAA,CAAA,MAAA;AACA,mBAAA,CAAA;AACA,qBAAA,UAAA,CAAA,IAAA,CAAA,IAAA;AACA;;AAEA,mBAAA,CAAA;AACA,qBAAA,YAAA,CAAA,IAAA,CAAA,IAAA;AACA;;AAEA,mBAAA,CAAA;AACA,qBAAA,WAAA,CAAA,IAAA,CAAA,IAAA;AACA;;AAEA;AACA;AAdA;AAgBA;AACA,SAxBA;AAyBA,OA5BA;AA6BA,KA5FA;AA6FA,IAAA,eAAA,EAAA,UAAA,IAAA,EAAA;AACA,UAAA,CAAA,GAAA,CAAA;AACA,UAAA,KAAA,GAAA,KAAA;AACA,UAAA,IAAA,GAAA,KAAA,gBAAA,CAAA,MAAA;;AAEA,aAAA,CAAA,GAAA,IAAA,IAAA,CAAA,KAAA,EAAA;AACA,YAAA,KAAA,gBAAA,CAAA,CAAA,KAAA,IAAA,EAAA;AACA,UAAA,KAAA,GAAA,IAAA;AACA,SAFA,MAEA;AACA,UAAA,CAAA;AACA;AACA;;AAEA,UAAA,KAAA,EAAA;AACA,eAAA,CAAA;AACA;AACA,KA7GA;AA8GA,IAAA,kBAAA,EAAA,UAAA,GAAA,EAAA;AACA,UAAA,CAAA,GAAA,CAAA;AACA,UAAA,KAAA,GAAA,KAAA;AACA,UAAA,IAAA,GAAA,IAAA,CAAA,WAAA,CAAA,MAAA;;AAEA,aAAA,CAAA,GAAA,IAAA,IAAA,CAAA,KAAA,EAAA;AACA,YAAA,IAAA,CAAA,WAAA,CAAA,CAAA,EAAA,YAAA,IAAA,GAAA,EAAA;AACA,UAAA,KAAA,GAAA,IAAA;AACA,SAFA,MAEA;AACA,UAAA,CAAA;AACA;AACA;;AAEA,UAAA,KAAA,EAAA;AACA,eAAA,IAAA,CAAA,WAAA,CAAA,CAAA,EAAA,UAAA;AACA;AACA;AA9HA,GAjCA;AAiKA,EAAA,OAAA,EAAA,YAAA;AACA,SAAA,MAAA;AACA;AAnKA,CAAA","sourcesContent":["<template>\n  <div class=\"try\">\n    <v-card class=\"cardConvocatoria\">\n      <div>\n        <h3 style=\"text-decoration: underline; margin-bottom: 10px\">{{ t }}</h3>\n      </div>\n      <div id=\"itemsCardConvocatoria\">\n        <div class=\"infoConvocatoria\">\n          <p style=\"margin-right: 10px; text-decoration: underline\">Día:</p>\n          <p>{{ d }}</p>\n        </div>\n        <div class=\"infoConvocatoria\">\n          <p style=\"margin-right: 10px; text-decoration: underline\">Hora:</p>\n          <p>{{ h }}</p>\n        </div>\n        <div class=\"infoConvocatoria\">\n          <p style=\"margin-right: 10px; text-decoration: underline\">Partido:</p>\n          <p>{{ m }}</p>\n        </div>\n      </div>\n    </v-card>\n\n    <div class=\"try2\">\n      <v-simple-table dense>\n        <template v-slot:default>\n          <thead>\n            <tr>\n              <th class=\"text-left\">- VOY ({{ playersVoy.length }}) -</th>\n            </tr>\n          </thead>\n          <tbody>\n            <tr v-for=\"i in playersVoy.length\" :key=\"i\">\n              <td style=\"text-align: center\">{{ playersVoy[i - 1] }}</td>\n            </tr>\n          </tbody>\n        </template>\n      </v-simple-table>\n\n      <v-simple-table dense>\n        <template v-slot:default>\n          <thead>\n            <tr>\n              <th class=\"text-left\">- NO VOY ({{ playersNoVoy.length }}) -</th>\n            </tr>\n          </thead>\n          <tbody>\n            <tr v-for=\"i in playersNoVoy.length\" :key=\"i\">\n              <td style=\"text-align: center\">{{ playersNoVoy[i - 1] }}</td>\n            </tr>\n          </tbody>\n        </template>\n      </v-simple-table>\n\n      <v-simple-table dense>\n        <template v-slot:default>\n          <thead>\n            <tr>\n              <th class=\"text-left\">- DUDA ({{ playersDuda.length }}) -</th>\n            </tr>\n          </thead>\n          <tbody>\n            <tr v-for=\"i in playersDuda.length\" :key=\"i\">\n              <td style=\"text-align: center\">{{ playersDuda[i - 1] }}</td>\n            </tr>\n          </tbody>\n        </template>\n      </v-simple-table>\n    </div>\n\n    <v-card class=\"cardConvocatoria\" style=\"width: 40vh\" :disabled=\"noMatch\">\n      <v-select\n        v-model=\"selectedPlayer\"\n        label=\"Escoger jugador\"\n        :items=\"playersListNames\"\n      ></v-select>\n\n      <v-row justify=\"center\">\n        <v-dialog\n          v-model=\"dialog\"\n          max-width=\"290\"\n          :disabled=\"!isselected\"\n        >\n          <template v-slot:activator=\"{ on, attrs }\">\n            <v-btn\n              color=\"primary\"\n              dark\n              style=\"width: 90%\"\n              v-bind=\"attrs\"\n              v-on=\"on\"\n              >Confirmar Asistencia</v-btn\n            >\n          </template>\n          <v-card>\n            <v-card-title class=\"headline\">{{ t2 }}</v-card-title>\n            <v-card-text>{{ st2 }}</v-card-text>\n\n            <v-col cols=\"12\" sm=\"12\">\n              <v-text-field\n                outlined\n                v-model=\"pass\"\n                label=\"Introduzca dorsal\"\n                prepend-icon=\"mdi-numeric\"\n                single-line\n                type=\"number\"\n              ></v-text-field>\n              <v-alert v-model=\"showError\" type=\"error\">{{\n                errorText\n              }}</v-alert>\n            </v-col>\n\n            <v-card-actions>\n              <v-spacer></v-spacer>\n              <v-btn color=\"green darken-1\" text @click=\"assist(0)\">Voy</v-btn>\n              <v-btn color=\"green darken-1\" text @click=\"assist(1)\"\n                >No Voy</v-btn\n              >\n            </v-card-actions>\n          </v-card>\n        </v-dialog>\n      </v-row>\n    </v-card>\n  </div>\n</template>\n\n<script>\n// @ is an alias to /src\n\nimport info from \"@/information\";\nimport fb from \"@/fb\";\n\nexport default {\n  props: [\"title\", \"day\", \"hour\", \"match\", \"title2\", \"subtitle2\", \"name\"],\n  watch: {\n    selectedPlayer: function () {\n      if (this.selectedPlayer != \"\") {\n        this.isselected = true;\n      } else {\n        this.isselected = false;\n      }\n    },\n  },\n  data: function () {\n    return {\n      t: this.title,\n      d: this.day,\n      h: this.hour,\n      m: this.match,\n      t2: this.title2,\n      st2: this.subtitle2,\n      pass: null,\n      showError: false,\n      errorText: \"Contraseña incorrecta\",\n      dialog: false,\n      playersListNames: [],\n      playersVoy: [],\n      playersNoVoy: [],\n      playersDuda: [],\n      playersIDsFB: [],\n      selectedPlayer: null,\n      isselected: false,\n      noMatch: null,\n    };\n  },\n  methods: {\n    checkDorsal: function () {\n      let found = false;\n      let i = 0;\n      let size = info.playersList.length;\n\n      while (i < size && !found) {\n        if (info.playersList[i].playerName == this.selectedPlayer) {\n          found = true;\n        } else {\n          i++;\n        }\n      }\n      if (found) {\n        if (info.playersList[i].playerNumber == this.pass) {\n          return true;\n        } else {\n          return false;\n        }\n      } else {\n        return false;\n      }\n    },\n    assist: function (type) {\n      this.showError = false;\n      let checked = this.checkDorsal();\n\n      if (checked) {\n        this.dialog = false;\n\n        if (type == 0) {\n          let index = this.lookforPosition(this.selectedPlayer);\n          console.log(index);\n\n          fb.db.collection(this.name).doc(\"J\" + this.$cookies.get(\"currentJ\")).collection(\"players\").doc(this.playersIDsFB[index]).update({\n            Status: 0,\n          });\n        } else if (type == 1) {\n          let index = this.lookforPosition(this.selectedPlayer);\n          fb.db.collection(this.name).doc(\"J\" + this.$cookies.get(\"currentJ\")).collection(\"players\").doc(this.playersIDsFB[index]).update({\n            Status: 1,\n          });\n        }\n\n        this.update();\n      } else {\n        this.showError = true;\n      }\n    },\n    update: function () {\n      this.playersListNames = [];\n      this.playersVoy = [];\n      this.playersNoVoy = [];\n      this.playersDuda = [];\n      this.playersIDsFB = [];\n      this.pass = \"\";\n\n      if (this.match == \"No disponible\") {\n        this.noMatch = true;\n      } else {\n        this.noMatch = false;\n      }\n\n      fb.db.collection(this.name).doc(\"J\" + this.$cookies.get('currentJ')).collection(\"players\").onSnapshot((res) => {\n        const changes = res.docChanges();\n\n        changes.forEach((change) => {\n          if (change.type === \"added\") {\n            let t = change.doc.data();\n              let name = this.lookforNamefromNum(t.PlayerNum);\n              this.playersListNames.push(name);\n              this.playersIDsFB.push(change.doc.id);\n\n              switch (t.Status) {\n                case 0:\n                  this.playersVoy.push(name);\n                  break;\n\n                case 1:\n                  this.playersNoVoy.push(name);\n                  break;\n\n                case 2:\n                  this.playersDuda.push(name);\n                  break;\n\n                default:\n                  break;\n              }\n            }\n        });\n      });\n    },\n    lookforPosition: function (name) {\n      let i = 0;\n      let found = false;\n      let size = this.playersListNames.length;\n\n      while (i < size && !found) {\n        if (this.playersListNames[i] == name) {\n          found = true;\n        } else {\n          i++;\n        }\n      }\n\n      if (found) {\n        return i;\n      }\n    },\n    lookforNamefromNum: function (num) {\n      let i = 0;\n      let found = false;\n      let size = info.playersList.length;\n\n      while (i < size && !found) {\n        if (info.playersList[i].playerNumber == num) {\n          found = true;\n        } else {\n          i++;\n        }\n      }\n\n      if (found) {\n        return info.playersList[i].playerName;\n      }\n    },\n  },\n  created: function () {\n    this.update();\n  },\n};\n</script>"],"sourceRoot":"src/components"}]}
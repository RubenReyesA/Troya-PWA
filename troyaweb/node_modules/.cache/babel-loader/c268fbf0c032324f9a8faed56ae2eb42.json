xt\">\n        <v-spacer></v-spacer>\n\n        <v-toolbar-title>Convocatoria</v-toolbar-title>\n\n        <v-spacer></v-spacer>\n      </v-toolbar>\n\n      <v-card class=\"cardConvocatoria\">\n        <div>\n          <h3 style=\"text-decoration:underline; margin-bottom:10px\">Datos de la Convocatoria</h3>\n        </div>\n        <div id=\"itemsCardConvocatoria\">\n          <div class=\"infoConvocatoria\">\n            <p style=\"margin-right: 10px; text-decoration:underline\">Día:</p>\n            <p>{{$cookies.get(\"currentDay\")}}</p>\n          </div>\n          <div class=\"infoConvocatoria\">\n            <p style=\"margin-right: 10px; text-decoration:underline\">Hora:</p>\n            <p>{{$cookies.get(\"currentHour\")}}</p>\n          </div>\n          <div class=\"infoConvocatoria\">\n            <p style=\"margin-right: 10px; text-decoration:underline\">Partido:</p>\n            <p>{{$cookies.get(\"currentMatch\")}}</p>\n          </div>\n        </div>\n      </v-card>\n\n      <div class=\"try2\">\n        <v-simple-table dense>\n          <template v-slot:default>\n            <thead>\n              <tr>\n                <th class=\"text-left\">- VOY ({{playersVoy.length}}) -</th>\n              </tr>\n            </thead>\n            <tbody>\n              <tr v-for=\"i in playersVoy.length\" :key=\"i\">\n                <td style=\"text-align:center\">{{playersVoy[i-1]}}</td>\n              </tr>\n            </tbody>\n          </template>\n        </v-simple-table>\n\n        <v-simple-table dense>\n          <template v-slot:default>\n            <thead>\n              <tr>\n                <th class=\"text-left\">- NO VOY ({{playersNoVoy.length}}) -</th>\n              </tr>\n            </thead>\n            <tbody>\n              <tr v-for=\"i in playersNoVoy.length\" :key=\"i\">\n                <td style=\"text-align:center\">{{playersNoVoy[i-1]}}</td>\n              </tr>\n            </tbody>\n          </template>\n        </v-simple-table>\n\n        <v-simple-table dense>\n          <template v-slot:default>\n            <thead>\n              <tr>\n                <th class=\"text-left\">- DUDA ({{playersDuda.length}}) -</th>\n              </tr>\n            </thead>\n            <tbody>\n              <tr v-for=\"i in playersDuda.length\" :key=\"i\">\n                <td style=\"text-align:center\">{{playersDuda[i-1]}}</td>\n              </tr>\n            </tbody>\n          </template>\n        </v-simple-table>\n      </div>\n\n      <v-card class=\"cardConvocatoria\" style=\"width:40vh\">\n        <v-select v-model=\"selectedPlayer\" label=\"Escoger jugador\" :items=\"playersListNames\"></v-select>\n\n        <v-row justify=\"center\">\n          <v-dialog v-model=\"dialog\" persistent max-width=\"290\" :disabled=\"!isselected\">\n            <template v-slot:activator=\"{ on, attrs }\">\n              <v-btn\n                color=\"primary\"\n                dark\n                style=\"width:90%\"\n                v-bind=\"attrs\"\n                v-on=\"on\"\n              >Confirmar Asistencia</v-btn>\n            </template>\n            <v-card>\n              <v-card-title class=\"headline\">Marca tu asistencia al partido</v-card-title>\n              <v-card-text>¿Vas a asistir al partido?</v-card-text>\n              <v-card-actions>\n                <v-spacer></v-spacer>\n                <v-btn color=\"green darken-1\" text @click=\"assist(0)\">Voy</v-btn>\n                <v-btn color=\"green darken-1\" text @click=\"assist(1)\">No Voy</v-btn>\n              </v-card-actions>\n            </v-card>\n          </v-dialog>\n        </v-row>\n      </v-card>\n    </div>\n  </div>\n</template>\n\n<script>\n// @ is an alias to /src\n\nimport info from \"@/information\";\nimport fb from \"@/fb\";\n\nexport default {\n  watch: {\n    selectedPlayer: function() {\n      if (this.selectedPlayer != \"\") {\n        this.isselected = true;\n      } else {\n        this.isselected = false;\n      }\n    }\n  },\n  data: function() {\n    return {\n      dialog: false,\n      playersListNames: [],\n      playersVoy: [],\n      playersNoVoy: [],\n      playersDuda: [],\n      playersIDsFB: [],\n      selectedPlayer: null,\n      isselected: false\n    };\n  },\n  methods: {\n    assist: function(type) {\n      this.dialog = false;\n\n      if (type == 0) {\n        let index = this.lookforPosition(this.selectedPlayer);\n        console.log(index);\n\n        fb.db\n          .collection(\"Convocatoria\")\n          .doc(this.playersIDsFB[index])\n          .update({\n            Status: 0\n          });\n      } else if (type == 1) {\n        let index = this.lookforPosition(this.selectedPlayer);\n        fb.db\n          .collection(\"Convocatoria\")\n          .doc(this.playersIDsFB[index])\n          .update({\n            Status: 1\n          });\n      }\n\n      this.update();\n    },\n    update: function() {\n      this.playersListNames = [];\n      this.playersVoy = [];\n      this.playersNoVoy = [];\n      this.playersDuda = [];\n      this.playersIDsFB = [];\n\n      let reset = false;\n      fb.db.collection(\"Convocatoria\").onSnapshot(res => {\n        const changes = res.docChanges();\n\n        let r = false;\n\n        changes.forEach(change => {\n          if (change.type === \"added\") {\n            let t = change.doc.data();\n\n            if (t.Type == \"0\") {\n              if (t.Jornada != this.$cookies.get(\"currentJ\")) {\n                r = true;\n                let p = {\n                  Type: \"0\",\n                  Jornada: this.$cookies.get(\"currentJ\")\n                };\n                fb.db\n                  .collection(\"Convocatoria\")\n                  .doc(change.doc.id)\n                  .update(p);\n              }\n            } else if (t.Type == \"1\") {\n              let name = this.lookforNamefromNum(t.PlayerNum);\n              this.playersListNames.push(name);\n              this.playersIDsFB.push(change.doc.id);\n\n              switch (t.Status) {\n                case 0:\n                  this.playersVoy.push(name);\n                  break;\n\n                case 1:\n                  this.playersNoVoy.push(name);\n                  break;\n\n                case 2:\n                  this.playersDuda.push(name);\n                  break;\n\n                default:\n                  break;\n              }\n            }\n          }\n        });\n        console.log(\"R: \" + r);\n        \n        this.reset = r;\n      });\n\n      console.log(reset);\n\n      if (reset) {\n        console.log(\"YES\");\n\n        fb.db.collection(\"Convocatoria\").onSnapshot(res => {\n          const changes = res.docChanges();\n\n          changes.forEach(change => {\n            if (change.type === \"added\") {\n              let t = change.doc.data();\n\n              if (t.Type == \"1\") {\n                let p = {\n                  PlayerNum: t.PlayerNum,\n                  Status: 2\n                };\n\n                fb.db\n                  .collection(\"Convocatoria\")\n                  .doc(change.doc.id)\n                  .update(p);\n              }\n            }\n          });\n        });\n        this.update();\n      }\n    },\n    add: function() {\n      for (let i = 0; i < info.playersList.length; i++) {\n        let p = {\n          PlayerNum: info.playersList[i].playerNumber,\n          Status: 2\n        };\n\n        fb.db.collection(\"Convocatoria\").add(p);\n      }\n    },\n    lookforPosition: function(name) {\n      let i = 0;\n      let found = false;\n      let size = this.playersListNames.length;\n\n      while (i < size && !found) {\n        if (this.playersListNames[i] == name) {\n          found = true;\n        } else {\n          i++;\n        }\n      }\n\n      if (found) {\n        return i;\n      }\n    },\n    lookforNamefromNum: function(num) {\n      let i = 0;\n      let found = false;\n      let size = info.playersList.length;\n\n      while (i < size && !found) {\n        if (info.playersList[i].playerNumber == num) {\n          found = true;\n        } else {\n          i++;\n        }\n      }\n\n      if (found) {\n        return info.playersList[i].playerName;\n      }\n    }\n  },\n  created: function() {\n    this.update();\n\n    this.day = info.currentInfo.currentDay;\n    this.hour = info.currentInfo.currentHour;\n    this.match = info.currentInfo.currentMatch;\n  }\n};\n</script>\n"],"sourceRoot":"src/views"}]}
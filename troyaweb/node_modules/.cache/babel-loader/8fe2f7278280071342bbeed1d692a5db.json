,IACA,KADA,GAEA,KAAA,OAAA,CAAA,KAAA,EAAA,OAAA,CAAA,KAAA,CAHA;AAKA,WAAA,QAAA,GAAA,IAAA;AACA;AAZA,GAbA;AA2BA,EAAA,OAAA,EAAA,mBAAA;AACA,SAAA,OAAA,GAAA,IAAA,CAAA,KAAA;AACA,SAAA,iBAAA;AACA,GA9BA;AA+BA,EAAA,OAAA,EAAA;AACA,IAAA,OAAA,EAAA,iBAAA,EAAA,EAAA;AACA,UAAA,CAAA,GAAA,EAAA;;AACA,cAAA,EAAA;AACA,aAAA,CAAA;AACA,UAAA,CAAA,GAAA,wBAAA,KAAA,cAAA;AAEA,UAAA,MAAA,CAAA,IAAA,CAAA,CAAA;AAEA;;AACA,aAAA,CAAA;AACA,UAAA,CAAA,GAAA,8BAAA,KAAA,YAAA,GAAA,MAAA,GAAA,KAAA,cAAA;AAEA,UAAA,MAAA,CAAA,IAAA,CAAA,CAAA;AAEA;;AACA,aAAA,CAAA;AACA,UAAA,CAAA,GAAA,gBAAA,KAAA,cAAA;AAEA,UAAA,MAAA,CAAA,IAAA,CAAA,CAAA;AAEA;;AAEA;AACA;AArBA;AAuBA,KA1BA;AA2BA,IAAA,iBAAA,EAAA,6BAAA;AACA,WAAA,IAAA,CAAA,GAAA,CAAA,EAAA,CAAA,GAAA,KAAA,OAAA,CAAA,MAAA,EAAA,CAAA,EAAA,EAAA;AACA,aAAA,YAAA,CAAA,IAAA,CAAA,KAAA,OAAA,CAAA,CAAA,EAAA,IAAA;AACA;AACA,KA/BA;AAgCA,IAAA,YAAA,EAAA,wBAAA;AACA,UAAA,CAAA,GAAA,CAAA;AACA,UAAA,KAAA,GAAA,CAAA;AACA,UAAA,IAAA,GAAA,KAAA,YAAA,CAAA,MAAA;;AAEA,aAAA,CAAA,GAAA,IAAA,IAAA,CAAA,KAAA,EAAA;AACA,YAAA,KAAA,YAAA,CAAA,CAAA,KAAA,KAAA,KAAA,EAAA;AACA,UAAA,KAAA,GAAA,IAAA;AACA,SAFA,MAEA;AACA,UAAA,CAAA;AACA;AACA;;AAEA,UAAA,KAAA,EAAA;AACA,eAAA,CAAA;AACA,OAFA,MAEA;AACA,eAAA,CAAA;AACA;AACA;AAlDA;AA/BA,CAAA","sourcesContent":["<template>\n  <div class=\"gps\">\n    <div class=\"try\">\n      <v-toolbar class=\"toolbarPage teal lighten-4 black--text\">\n        <v-spacer></v-spacer>\n\n        <v-toolbar-title>Campos de Fútbol (GPS)</v-toolbar-title>\n\n        <v-spacer></v-spacer>\n      </v-toolbar>\n\n      <v-card id=\"gpsframe\">\n        <v-select\n          v-model=\"value\"\n          label=\"Campos de Fútbol\"\n          :items=\"gpsListNames\"\n          style=\"width: 70vw\"\n        ></v-select>\n\n        <div id=\"gpsData\">\n          <p style=\"margin-right: 10px; text-decoration: underline\">Nombre:</p>\n          <p>{{ selectedName }}</p>\n          <p style=\"margin-right: 10px; text-decoration: underline\">\n            Dirección:\n          </p>\n          <p>{{ selectedLocation }}</p>\n          <p style=\"margin-right: 10px; text-decoration: underline\">\n            Coordenadas:\n          </p>\n          <p>{{ selectedCoords }}</p>\n        </div>\n\n        <v-alert type=\"info\" id=\"infoGO\">{{ comment }}</v-alert>\n\n        <div id=\"btnLayout\">\n          <v-btn\n            class=\"btnMaps\"\n            color=\"secondary\"\n            large\n            :disabled=\"!selected\"\n            @click=\"driveTo(1)\"\n            >Google Maps</v-btn\n          >\n          <v-btn\n            class=\"btnMaps\"\n            color=\"secondary\"\n            large\n            :disabled=\"!selected\"\n            @click=\"driveTo(2)\"\n            >Apple Maps</v-btn\n          >\n          <v-btn\n            class=\"btnMaps\"\n            color=\"secondary\"\n            large\n            :disabled=\"!selected\"\n            @click=\"driveTo(3)\"\n            >Waze</v-btn\n          >\n          <v-btn\n          class=\"btnMaps\"\n            color=\"secondary\"\n            large\n            :disabled=\"!selected\"\n            @click=\"driveTo(4)\">Otros</v-btn>\n        </div>\n      </v-card>\n    </div>\n  </div>\n</template>\n\n<script>\n// @ is an alias to /src\n\nimport info from \"@/information\";\n\nexport default {\n  data: function () {\n    return {\n      value: \"\",\n      selected: false,\n      gpsList: [],\n      gpsListNames: [],\n      comment: \"Selecciona el navegador GPS preferido\",\n      selectedName: \"\",\n      selectedLocation: \"\",\n      selectedCoords: \"\",\n    };\n  },\n  watch: {\n    value: function () {\n      let index = this.lookforIndex();\n      this.selectedName = this.gpsList[index].stadium[\"name\"];\n      this.selectedLocation = this.gpsList[index].stadium[\"location\"];\n\n      this.selectedCoords =\n        this.gpsList[index].stadium[\"lat\"] +\n        \" , \" +\n        this.gpsList[index].stadium[\"lon\"];\n\n      this.selected = true;\n    },\n  },\n  created: function () {\n    this.gpsList = info.teams;\n    this.generateListNames();\n  },\n  methods: {\n    driveTo: function (id) {\n      let s = \"\";\n      switch (id) {\n        case 1:\n          s = \"comgooglemaps://?q=\" + this.selectedCoords;\n\n          window.open(s);\n\n          break;\n        case 2:\n          s = \"http://maps.apple.com/?q=\"+ this.selectedName + \"&ll=\" + this.selectedCoords;\n\n          window.open(s);\n\n          break;\n        case 3:\n          s = \"waze://?ll=\" + this.selectedCoords;\n\n          window.open(s);\n\n          break;\n\n        default:\n          break;\n      }\n    },\n    generateListNames: function () {\n      for (let i = 0; i < this.gpsList.length; i++) {\n        this.gpsListNames.push(this.gpsList[i].name);\n      }\n    },\n    lookforIndex: function () {\n      let i = 0;\n      let found = 0;\n      let size = this.gpsListNames.length;\n\n      while (i < size && !found) {\n        if (this.gpsListNames[i] == this.value) {\n          found = true;\n        } else {\n          i++;\n        }\n      }\n\n      if (found) {\n        return i;\n      } else {\n        return 0;\n      }\n    },\n  },\n};\n</script>\n"],"sourceRoot":"src/views"}]}
,KAAA,EAAA,OAAA,CAAA,UAAA,CAAA;AAEA,WAAA,cAAA,GACA,KAAA,OAAA,CAAA,KAAA,EAAA,OAAA,CAAA,KAAA,IACA,KADA,GAEA,KAAA,OAAA,CAAA,KAAA,EAAA,OAAA,CAAA,KAAA,CAHA;AAKA,WAAA,eAAA,GAAA,KAAA,OAAA,CAAA,KAAA,EAAA,OAAA,CAAA,UAAA,CAAA;AAEA,WAAA,KAAA,GAAA,OAAA,CAAA,yBAAA,KAAA,GAAA,CAAA,IAAA,MAAA,CAAA;AACA,WAAA,KAAA,GAAA,OAAA,CAAA,yBAAA,KAAA,GAAA,CAAA,IAAA,MAAA,CAAA;AAEA,WAAA,QAAA,GAAA,IAAA;AACA;AAjBA,GAhBA;AAmCA,EAAA,OAAA,EAAA,mBAAA;AACA,SAAA,OAAA,GAAA,IAAA,CAAA,KAAA;AACA,SAAA,iBAAA;AACA,GAtCA;AAuCA,EAAA,OAAA,EAAA;AACA,IAAA,OAAA,EAAA,iBAAA,EAAA,EAAA;AACA,UAAA,CAAA,GAAA,EAAA;;AACA,cAAA,EAAA;AACA,aAAA,CAAA;AACA;AACA,UAAA,CAAA,GACA,qDACA,KAAA,cADA,GAEA,kBAFA,GAGA,KAAA,eAJA;AAMA,UAAA,MAAA,CAAA,IAAA,CAAA,CAAA;AAEA;;AACA,aAAA,CAAA;AACA,UAAA,CAAA,GACA,8BACA,KAAA,YADA,GAEA,MAFA,GAGA,KAAA,cAJA;AAMA,UAAA,MAAA,CAAA,IAAA,CAAA,CAAA;AAEA;;AACA,aAAA,CAAA;AACA,UAAA,CAAA,GAAA,gBAAA,KAAA,cAAA;AAEA,UAAA,MAAA,CAAA,IAAA,CAAA,CAAA;AAEA;;AACA,aAAA,CAAA;AACA,UAAA,CAAA,GAAA,SAAA,KAAA,cAAA;AAEA,UAAA,MAAA,CAAA,IAAA,CAAA,CAAA;AAEA;;AACA;AACA;AAnCA;AAqCA,KAxCA;AAyCA,IAAA,iBAAA,EAAA,6BAAA;AACA,WAAA,IAAA,CAAA,GAAA,CAAA,EAAA,CAAA,GAAA,KAAA,OAAA,CAAA,MAAA,EAAA,CAAA,EAAA,EAAA;AACA,aAAA,YAAA,CAAA,IAAA,CAAA,KAAA,OAAA,CAAA,CAAA,EAAA,IAAA;AACA;AACA,KA7CA;AA8CA,IAAA,YAAA,EAAA,wBAAA;AACA,UAAA,CAAA,GAAA,CAAA;AACA,UAAA,KAAA,GAAA,CAAA;AACA,UAAA,IAAA,GAAA,KAAA,YAAA,CAAA,MAAA;;AAEA,aAAA,CAAA,GAAA,IAAA,IAAA,CAAA,KAAA,EAAA;AACA,YAAA,KAAA,YAAA,CAAA,CAAA,KAAA,KAAA,KAAA,EAAA;AACA,UAAA,KAAA,GAAA,IAAA;AACA,SAFA,MAEA;AACA,UAAA,CAAA;AACA;AACA;;AAEA,UAAA,KAAA,EAAA;AACA,eAAA,CAAA;AACA,OAFA,MAEA;AACA,eAAA,CAAA;AACA;AACA;AAhEA;AAvCA,CAAA","sourcesContent":["<template>\n  <div class=\"gps\">\n    <div class=\"try\">\n      <v-toolbar class=\"toolbarPage teal lighten-4 black--text\">\n        <v-spacer></v-spacer>\n\n        <v-toolbar-title>Campos de Fútbol (GPS)</v-toolbar-title>\n\n        <v-spacer></v-spacer>\n      </v-toolbar>\n\n      <v-card id=\"gpsframe\">\n        <v-select\n          v-model=\"value\"\n          label=\"Campos de Fútbol\"\n          :items=\"gpsListNames\"\n          style=\"width: 70vw\"\n        ></v-select>\n\n        <div class=\"try\">\n          <div class=\"gpsData\">\n            <p style=\"margin-right: 10px; text-decoration: underline\">\n              Nombre:\n            </p>\n            <p>{{ selectedName }}</p>\n            <p style=\"margin-right: 10px; text-decoration: underline\">\n              Dirección:\n            </p>\n            <p>{{ selectedLocation }}</p>\n            <p style=\"margin-right: 10px; text-decoration: underline\">\n              Coordenadas:\n            </p>\n            <p>{{ selectedCoords }}</p>\n          </div>\n          <div class=\"clothesData\">\n            <div class=\"clothesImages\">\n              <v-img class=\"cPict\" :src=\"shirt\"> </v-img>\n              <p style=\"color: white\">kkkkkk</p>\n              <v-img class=\"cPict\" :src=\"pants\"> </v-img>\n            </div>\n            <p id=\"text\">Vestimenta</p>\n          </div>\n        </div>\n\n        <v-alert type=\"info\" id=\"infoGO\">{{ comment }}</v-alert>\n\n        <div id=\"btnLayout\">\n          <v-btn\n            class=\"btnMaps\"\n            color=\"secondary\"\n            large\n            :disabled=\"!selected\"\n            @click=\"driveTo(1)\"\n            >Google Maps</v-btn\n          >\n          <v-btn\n            class=\"btnMaps\"\n            color=\"secondary\"\n            large\n            :disabled=\"!selected\"\n            @click=\"driveTo(2)\"\n            >Apple Maps (iOS)</v-btn\n          >\n          <v-btn\n            class=\"btnMaps\"\n            color=\"secondary\"\n            large\n            :disabled=\"!selected\"\n            @click=\"driveTo(3)\"\n            >Waze</v-btn\n          >\n          <v-btn\n            class=\"btnMaps\"\n            color=\"secondary\"\n            large\n            :disabled=\"!selected\"\n            @click=\"driveTo(4)\"\n            >Otros (Android)</v-btn\n          >\n        </div>\n      </v-card>\n    </div>\n  </div>\n</template>\n\n<script>\n// @ is an alias to /src\n\nimport info from \"@/information\";\n\nexport default {\n  data: function () {\n    return {\n      value: \"\",\n      selected: false,\n      gpsList: [],\n      gpsListNames: [],\n      comment: \"Selecciona el navegador GPS preferido\",\n      selectedName: \"\",\n      selectedLocation: \"\",\n      selectedCoords: \"\",\n      selectedPlaceID: \"\",\n      shirt: \"\",\n      pants: \"\",\n    };\n  },\n  watch: {\n    value: function () {\n      let index = this.lookforIndex();\n      this.selectedName = this.gpsList[index].stadium[\"name\"];\n      this.selectedLocation = this.gpsList[index].stadium[\"location\"];\n\n      this.selectedCoords =\n        this.gpsList[index].stadium[\"lat\"] +\n        \" , \" +\n        this.gpsList[index].stadium[\"lon\"];\n\n      this.selectedPlaceID = this.gpsList[index].stadium[\"place_id\"];\n\n      this.shirt = require(\"../assets/Clothes/s\" + (index+1) + \".png\");\n      this.pants = require(\"../assets/Clothes/p\" + (index+1) + \".png\");\n\n      this.selected = true;\n    },\n  },\n  created: function () {\n    this.gpsList = info.teams;\n    this.generateListNames();\n  },\n  methods: {\n    driveTo: function (id) {\n      let s = \"\";\n      switch (id) {\n        case 1:\n          //s = \"comgooglemaps://?q=\" + this.selectedCoords;\n          s =\n            \"https://www.google.com/maps/search/?api=1&query=\" +\n            this.selectedCoords +\n            \"&query_place_id=\" +\n            this.selectedPlaceID;\n\n          window.open(s);\n\n          break;\n        case 2:\n          s =\n            \"http://maps.apple.com/?q=\" +\n            this.selectedName +\n            \"&ll=\" +\n            this.selectedCoords;\n\n          window.open(s);\n\n          break;\n        case 3:\n          s = \"waze://?ll=\" + this.selectedCoords;\n\n          window.open(s);\n\n          break;\n        case 4:\n          s = \"geo:\" + this.selectedCoords;\n\n          window.open(s);\n\n          break;\n        default:\n          break;\n      }\n    },\n    generateListNames: function () {\n      for (let i = 0; i < this.gpsList.length; i++) {\n        this.gpsListNames.push(this.gpsList[i].name);\n      }\n    },\n    lookforIndex: function () {\n      let i = 0;\n      let found = 0;\n      let size = this.gpsListNames.length;\n\n      while (i < size && !found) {\n        if (this.gpsListNames[i] == this.value) {\n          found = true;\n        } else {\n          i++;\n        }\n      }\n\n      if (found) {\n        return i;\n      } else {\n        return 0;\n      }\n    },\n  },\n};\n</script>\n"],"sourceRoot":"src/views"}]}
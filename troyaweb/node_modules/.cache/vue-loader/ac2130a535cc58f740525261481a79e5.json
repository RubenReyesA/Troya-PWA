    <v-tab-item value=\"tab2\">\n          <v-card id=\"saldoCard\">\n            <h4>Partidos: {{count}}/30</h4>\n          </v-card>\n\n          <v-simple-table style=\"width:40vw\">\n            <template v-slot:default>\n              <thead>\n                <tr>\n                  <th class=\"text-align:center\">Jugador</th>\n                  <th class=\"text-align:center\">Reyes</th>\n                </tr>\n              </thead>\n              <tbody>\n                <tr v-for=\"item in infoRow\" :key=\"item.name\">\n                  <td>{{ item.name }}</td>\n                  <td @click=\"mark(item.name)\" style=\"cursor:pointer\">{{ item.j}}</td>\n                </tr>\n              </tbody>\n            </template>\n          </v-simple-table>\n        </v-tab-item>\n      </v-tabs-items>\n    </div>\n  </div>\n</template>\n\n<script>\n// @ is an alias to /src\n\nexport default {\n  data: function() {\n    return {\n      infoRow: null,\n      count: 0,\n      tabs: null\n    };\n  },\n  methods: {\n    mark: function(name) {\n      let index = this.lookForIndex(name);\n\n      if (this.infoRow[index].j == \"\") {\n        this.infoRow[index].j = \"X\";\n        this.count++;\n      } else {\n        this.infoRow[index].j = \"\";\n        this.count--;\n      }\n    },\n    lookForIndex: function(name) {\n      let i = 0;\n      let found = false;\n      let size = this.infoRow.length;\n\n      while (i < size && !found) {\n        if (name == this.infoRow[i].name) {\n          found = true;\n        } else {\n          i++;\n        }\n      }\n\n      if (found) {\n        return i;\n      }\n    }\n  },\n  created: function() {\n    this.infoRow = [];\n\n    for (let i = 1; i <= 30; i++) {\n      let p = {\n        name: \"J\" + i,\n        j: \"\"\n      };\n      this.infoRow.push(p);\n    }\n  }\n};\n</script>\n"]}]}